{
  "compilerOptions": {
    "target": "es2021",
    "module": "es2022",
    "moduleResolution": "Bundler",
    "lib": ["es2021"],
    "types": [
      "vitest-globals",
      "@cloudflare/workers-types",
      "@cloudflare/vitest-pool-workers"
    ],
    "isolatedModules": true,
    "resolveJsonModule": true,
    "strict": true,
    "allowJs": true,
    "checkJs": false,
    "noEmit": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "jsx": "react"
  },
  "exclude": ["test"]
}
